{
  "name": "react-web-templete-pjy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.3.0",
    "axios": "^1.3.3",
    "classnames": "^2.3.2",
    "compression-webpack-plugin": "^10.0.0",
    "craco-less": "^2.0.0",
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set PORT=5051 && cross-env PROXY_ENV=dev craco start",
    "start:dev": "set PORT=5051 && cross-env PROXY_ENV=dev craco start",
    "start:st": "cross-env PROXY_ENV=st craco start",
    "start:uat": "cross-env PROXY_ENV=uat craco start",
    "build": "craco build",
    "test": "jest --config",
    "coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "globals": {
      "__webpack_public_path__": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@craco/craco": "^7.0.0",
    "@types/jest": "^29.5.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-less-loader": "^0.2.0",
    "lint-staged": "^13.1.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.1.0",
    "ts-jest": "^29.1.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.less": [
      "stylelint --fix --custom-syntax postcss-less",
      "git add"
    ]
  }
}
